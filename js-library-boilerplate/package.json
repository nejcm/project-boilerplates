{
  "name": "@nejcm/js-library-boilerplate",
  "version": "0.0.0-semantically-released",
  "author": "Nejc Mursic <nmursi2@gmail.com> (https://github.com/nejcm)",
  "description": "Starter kit for your js library projects.",
  "source": "src/index.js",
  "main": "dist/@nejcm/js-library-boilerplate.cjs.js",
  "umd:main": "dist/@nejcm/js-library-boilerplate.umd.js",
  "module": "dist/@nejcm/js-library-boilerplate.esm.js",
  "types": "types/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nejcm/js-library-boilerplate"
  },
  "bugs": {
    "url": "https://github.com/nejc/js-library-boilerplate/issues"
  },
  "homepage": "https://github.com/nejcm/js-library-boilerplate",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "keywords": [
    "js",
    "es6",
    "react",
    "library"
  ],
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "storybook": "start-storybook -p 9001 -c .storybook",
    "format": "prettier ./src/**/*.js --write",
    "clean": "rimraf dist coverage",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "test": "npm run lint && jest",
    "test:cov": "jest --coverage --colors && jest-coverage-badges",
    "test:build": "jest --coverage --colors --silent && jest-coverage-badges",
    "test:watch": "jest --watch",
    "docs:dev": "docz dev",
    "docs:build": "docz build",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch",
    "tsc:declaration": "tsc -p tsconfig.json --emitDeclarationOnly",
    "docs:serve": "serve ./.docz/dist --listen 6006 --single",
    "build": "npm run clean && kcd-scripts build --bundle --no-clean --size-snapshot",
    "validate": "npm run format && npm run lint && npm run test:build",
    "setup": "npm install && npm run validate",
    "semantic-release": "semantic-release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "peerDependencies": {},
  "dependencies": {
    "@babel/runtime": "^7.9.2"
  },
  "devDependencies": {
    "@babel/compat-data": "^7.11.0",
    "@babel/helpers": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.7",
    "@semantic-release/npm": "^7.0.5",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@storybook/addon-actions": "^6.0.12",
    "@storybook/addon-docs": "^6.0.12",
    "@storybook/addon-knobs": "^6.0.12",
    "@storybook/addon-links": "^6.0.12",
    "@storybook/addon-notes": "^5.3.19",
    "@storybook/addon-options": "^5.3.19",
    "@storybook/addons": "^6.0.12",
    "@storybook/react": "^6.0.12",
    "@testing-library/jest-dom": "^5.11.3",
    "@testing-library/react": "^10.4.8",
    "@testing-library/react-hooks": "^3.4.1",
    "@testing-library/user-event": "^12.1.1",
    "@types/jest": "^26.0.10",
    "@types/react": "^16.9.46",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.1.0",
    "babel-plugin-minify-dead-code-elimination": "^0.5.1",
    "babel-plugin-no-side-effect-class-properties": "^0.0.7",
    "babel-plugin-transform-async-to-promises": "^0.8.15",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "codecov": "^3.7.2",
    "cpy-cli": "^3.1.1",
    "cross-env": "^7.0.2",
    "docz": "^2.3.1",
    "docz-theme-default": "^1.2.0",
    "eslint": "^7.7.0",
    "eslint-config-kentcdodds": "^14.14.2",
    "eslint-config-react-app": "^5.2.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-html": "^6.0.3",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "jest": "^26.4.0",
    "jest-coverage-badges": "^1.1.2",
    "jest-environment-jsdom-fourteen": "^1.0.1",
    "jest-resolve": "^26.4.0",
    "jest-watch-typeahead": "^0.6.0",
    "kcd-scripts": "^6.2.4",
    "lint-staged": "^10.2.11",
    "node-sass": "^4.14.1",
    "postcss": "^7.0.32",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.26.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-sass": "^1.2.2",
    "sass-loader": "^9.0.3",
    "semantic-release": "^17.1.1",
    "serve": "^11.3.2",
    "start-server-and-test": "^1.11.3",
    "style-loader": "^1.2.1",
    "typescript": "^3.9.7"
  },
  "browserslist": [
    "last 2 version",
    "> 1%",
    "IE 11",
    "safari >= 7"
  ]
}
